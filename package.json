{
  "name": "@kuzen/atom-one-light",
  "version": "0.0.1",
  "description": " Atom One Light for JupyterLab",
  "author": "kuzen <jldjz97@outlook.com>",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme"
  ],
  "license": "MIT",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*"
  ],
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf static && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "~0.21.2",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "style/index.css"
  },
  "homepage": "https://github.com/kuzen/jupyterlab_atom_one_light",
  "bugs": {
    "url": "https://github.com/kuzen/jupyterlab_atom_one_light/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kuzen/jupyterlab_atom_one_light.git"
  }
}
